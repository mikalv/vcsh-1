(use-package! "unit-test")

(define-test numeric-simple-comparison
  (test-case (> 2 1))
  (test-case (not (> 1 1)))
  (test-case (not (> 1 2)))
  (test-case (>= 2 1))
  (test-case (>= 1 1))
  (test-case (not (>= 1 2)))
  (test-case (< 1 2))
  (test-case (not (< 1 1)))
  (test-case (not (< 2 1)))
  (test-case (<= 1 2))
  (test-case (<= 1 1))
  (test-case (not (<= 2 1)))

  (test-case (> 2.0 1.0))
  (test-case (not (> 1.0 1.0)))
  (test-case (not (> 1.0 2.0)))
  (test-case (>= 2.0 1.0))
  (test-case (>= 1.0 1.0))
  (test-case (not (>= 1.0 2.0)))
  (test-case (< 1.0 2.0))
  (test-case (not (< 1.0 1.0)))
  (test-case (not (< 2.0 1.0)))
  (test-case (<= 1.0 2.0))
  (test-case (<= 1.0 1.0))
  (test-case (not (<= 2.0 1.0)))

  (test-case (> 2.0 1))
  (test-case (not (> 1.0 1)))
  (test-case (not (> 1.0 2)))
  (test-case (>= 2.0 1))
  (test-case (>= 1.0 1))
  (test-case (not (>= 1.0 2)))
  (test-case (< 1.0 2))
  (test-case (not (< 1.0 1)))
  (test-case (not (< 2.0 1)))
  (test-case (<= 1.0 2))
  (test-case (<= 1.0 1))
  (test-case (not (<= 2.0 1)))
  (test-case (> 2 1.0))
  (test-case (not (> 1 1.0)))
  (test-case (not (> 1 2.0)))
  (test-case (>= 2 1.0))
  (test-case (>= 1 1.0))
  (test-case (not (>= 1 2.0)))
  (test-case (< 1 2.0))
  (test-case (not (< 1 1.0)))
  (test-case (not (< 2 1.0)))
  (test-case (<= 1 2.0))
  (test-case (<= 1 1.0))
  (test-case (not (<= 2 1.0)))

  (test-case (> 8 4 2))
  (test-case (not (> 3 4 2)))
  (test-case (not (> 8 9 2)))
  (test-case (not (> 8 4 5)))
  (test-case (not (> 2 4 8)))

  (test-case (>= 8 4 2))
  (test-case (>= 8 8 2))
  (test-case (>= 8 4 4))
  (test-case (>= 8 8 8)		)
  (test-case (not (>= 3 4 2)))
  (test-case (not (>= 8 9 2)))
  (test-case (not (>= 8 4 5)))
  (test-case (not (>= 2 4 8)))
  (test-case (not (>= 4 4 8)))
  (test-case (not (>= 8 9 9)))

  (test-case (< 2 4 8))
  (test-case (not (< 2 4 3)))
  (test-case (not (< 2 9 8)))
  (test-case (not (< 5 4 8)))
  (test-case (not (< 8 4 2)))

  (test-case (<= 2 4 8))
  (test-case (<= 2 8 8))
  (test-case (<= 4 4 8))
  (test-case (<= 8 8 8)		)
  (test-case (not (<= 2 4 3)))
  (test-case (not (<= 2 9 8)))
  (test-case (not (<= 5 4 8)))
  (test-case (not (<= 8 4 2)))
  (test-case (not (<= 8 4 4)))
  (test-case (not (<= 9 9 8)))

  (test-case (> 8.0 4.0 2.0))
  (test-case (not (> 3.0 4.0 2.0)))
  (test-case (not (> 8.0 9.0 2.0)))
  (test-case (not (> 8.0 4.0 5.0)))
  (test-case (not (> 2.0 4.0 8.0)))

  (test-case (>= 8.0 4.0 2.0))
  (test-case (>= 8.0 8.0 2.0))
  (test-case (>= 8.0 4.0 4.0))
  (test-case (>= 8.0 8.0 8.0)		)
  (test-case (not (>= 3.0 4.0 2.0)))
  (test-case (not (>= 8.0 9.0 2.0)))
  (test-case (not (>= 8.0 4.0 5.0)))
  (test-case (not (>= 2.0 4.0 8.0)))
  (test-case (not (>= 4.0 4.0 8.0)))
  (test-case (not (>= 8.0 9.0 9.0)))

  (test-case (< 2.0 4.0 8.0))
  (test-case (not (< 2.0 4.0 3.0)))
  (test-case (not (< 2.0 9.0 8.0)))
  (test-case (not (< 5.0 4.0 8.0)))
  (test-case (not (< 8.0 4.0 2.0)))

  (test-case (<= 2.0 4.0 8.0))
  (test-case (<= 2.0 8.0 8.0))
  (test-case (<= 4.0 4.0 8.0))
  (test-case (<= 8.0 8.0 8.0)		)
  (test-case (not (<= 2.0 4.0 3.0)))
  (test-case (not (<= 2.0 9.0 8.0)))
  (test-case (not (<= 5.0 4.0 8.0)))
  (test-case (not (<= 8.0 4.0 2.0)))
  (test-case (not (<= 8.0 4.0 4.0)))
  (test-case (not (<= 9.0 9.0 8.0))))
