(use-package! "unit-test")

(define-test string-search-from-right
  (test-case (equal? 2  (string-search-from-right "345" "1234567890")))
  (test-case (equal? #f (string-search-from-right "34567890a" "1234567890")))
  (test-case (equal? #f (string-search-from-right "hello world" "1234567890")))
  (test-case (equal? 0  (string-search-from-right "1" "1234567890")))
  (test-case (equal? 9  (string-search-from-right "0" "1234567890")))
  (test-case (equal? 6  (string-search-from-right "2" "1234512345")))
  (test-case (equal? #f (string-search-from-right "2" "1234512345" 0)))
  (test-case (equal? 1  (string-search-from-right "2" "1234512345" 4)))
  (test-case (equal? 1  (string-search-from-right "234" "1234512345" 4)))
  (test-case (equal? #f (string-search-from-right "234" "1234512345" 1)))
  (test-case (equal? 1  (string-search-from-right "234" "1234512345" 6)))
  (test-case (equal? #f (string-search-from-right "234" "1234512345" 60)))
  (test-case (equal? 6  (string-search-from-right "234" "1234512345" 8)))
  (test-case (equal? 12 (string-search-from-right "a" "abaabaaabaaaab"))   )
  (test-case (equal? 12 (string-search-from-right "ab" "abaabaaabaaaab")))
  (test-case (equal? 10 (string-search-from-right "aaa"  "abaabaaabaaaab"))   )
  (test-case (equal? 10 (string-search-from-right "aaab" "abaabaaabaaaab")))

  (test-case (equal? 1  (string-search-from-right "aaab" "aaaab")))
  (test-case (equal? #f  (string-search-from-right "aaab" "aaaab" 1)))
  (test-case (equal? #f  (string-search-from-right "aaab" "aaaab" 2)))

  (test-case (equal? 0  (string-search-from-right #\1 "1234567890")))
  (test-case (equal? 9  (string-search-from-right #\0 "1234567890")))
  (test-case (equal? 6  (string-search-from-right #\2 "1234512345")))
  (test-case (equal? #f (string-search-from-right #\2 "1234512345" 0)))
  (test-case (equal? 1  (string-search-from-right #\2 "1234512345" 4)))
  (test-case (equal? 12 (string-search-from-right #\a "abaabaaabaaaab"))))
